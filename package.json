{
  "name": "employee-management-system",
  "version": "0.0.1",
  "description": "Applications",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run prettier:check && yarn run eslint",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint --fix",
    "pretest": "yarn run clean && yarn run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "yarn run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && yarn run posttest",
    "docker:build": "docker build -t employee-management-system .",
    "docker:run": "docker run -p 3000:3000 -d employee-management-system",
    "migrate": "node ./dist/migrate",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "yarn run build",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "repository": {
    "type": "git"
  },
  "author": "sabulous <sabsah@gmail.com>",
  "license": "UNLICENSED",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^6.0.1",
    "@loopback/authentication-jwt": "^0.6.0",
    "@loopback/boot": "^2.5.1",
    "@loopback/core": "^2.9.5",
    "@loopback/extension-logging": "^0.3.13",
    "@loopback/repository": "^2.11.2",
    "@loopback/rest": "^6.2.0",
    "@loopback/rest-explorer": "^2.2.10",
    "@loopback/service-proxy": "^2.3.8",
    "loopback-connector-postgresql": "^5.1.0",
    "loopback-connector-rest": "^3.2.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.2",
    "@loopback/eslint-config": "^9.0.2",
    "@loopback/testlab": "^3.2.4",
    "@types/node": "^10.17.28",
    "eslint": "^7.7.0",
    "mocha": "^8.1.3",
    "source-map-support": "^0.5.19",
    "typescript": "~4.0.2"
  }
}
